<?xml version='1.0' encoding = 'ISO-8859-1' ?> 

<definitions name='WS_FGA'
	targetNamespace = 'urn:WS_FGA'
	xmlns:tns = 'http://WS_FGA.org/WS_FGA'
	xmlns:soap = 'http://schemas.xmlsoap.org/wsdl/soap/'
	xmlns:xsd = 'http://www.w3.org/2001/XMLSchemas'
	xmlns:soapenc = 'http://schemas.xmlsoap.org/soap/encoding/'
	xmlns:wsdl = 'http://schemas.xmlsoap.org/wsdl/'
	xmlns = 'http://schemas.xmlsoap.org/wsdl/' >
	
	
	
<message name='getNomeRequest'>
	<part name='codigo' type='xsd:string' />
</message>
<message name = 'getNomeResponse'>
	<part name='resultado' type='xsd:string' />
</message>



<portType name='WS_FGAPortType'>
	<operation name='getNome'>
		<input message='tns:getNomeRequest' />
		<output message='tns:getNomeResponse' />
	</operation>
</portType>



<binding name='WS_FGABinding' type='tns:WS_FGAPortType'>
	<soap:binding style='rpc' transport='http://schemas.xmlsoap.org/soap/http' />
	<operation name='getNome'>
		
		<soap:operation soapAction='urn:xmethods-delayed-ws_fga#getNome' />
		<input>
			<soap:body use='enconded' namespace='urn:xmethods-delayed-ws_fga'
			encodingStyle='http://schemas.xmlsoap.org/soap/encoding/' />
		</input>
		<output>
			<soap:body use='encoded' namespace='urn:xmethods-delayed-ws_fga' 
			encodingStyle='http://schemas.xmlsoap.org/soap/encoding/' />
		</output>
	</operation>
</binding>


<service name='WS_FGAService'>
	<port name='WS_FGAPort' binding='WS_FGABinding'>
		<soap:address location='http://localhost/WsFGA-AppMobile/app/ws/servidor.php' />
	</port>
</service>

</definitions>
